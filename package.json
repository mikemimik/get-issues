{
  "name": "get-issues",
  "version": "0.0.4",
  "description": "cli that downloads all github issues from project for offline use",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "publish": "npm run git:add && npm run git:message && npm run git:push && npm run npm:publish",
    "git:add": "git add .",
    "git:message": "git commit -m 'update gh-issues' ",
    "git:push": "git push origin master",
    "npm:publish": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pippinlee/get-issues.git"
  },
  "keywords": [
    "git",
    "issues",
    "offline",
    "github"
  ],
  "author": "@pippinlee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pippinlee/get-issues/issues"
  },
  "homepage": "https://github.com/pippinlee/get-issues#readme",
  "bin": {
    "get-issues": "src/index.js"
  },
  "dependencies": {
    "async": "^1.5.0",
    "gh-api": "^1.0.0",
    "request": "^2.65.0"
  }
}
